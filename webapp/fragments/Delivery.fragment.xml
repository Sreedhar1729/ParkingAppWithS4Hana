<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns="sap.m"
>
    <Dialog
        id="idEditingslot"
        title="Slot Delivery  Change"
    >
        <beginButton>
            <Button
                id="sloteditbutton55"
                text="Save"
                press="onSaves"
                type="Emphasized"
            />
        </beginButton>
        <endButton>
            <Button
                id="ideditscancel"
                text="Cancel"
                press="onEditCancel"
                type="Negative"
            />
        </endButton>
        <content>
            <f:SimpleForm id="idformedit">
                <f:content>
                    <Label
                        id="idSLotnumberedit"
                        text="Slot Number:"
                        width="55%"
                    />
                    <Text
                        id="idSlotnumbervalue"
                        text="{  }"
                        width="55%"
                    />
                    <Label
                        id="iddeliveryteypeedit"
                        text="Delivery"
                        width="55%"
                    />
                    <Select
                        id="inwards2111"
                        required="true"
                        selectedKey="{Delivery}"
                        change="onDeliveryChange1"
                    >
                        <items>
                            <core:Item
                                id="iditeadms1211"
                                text=" "
                                key=""
                            />
                            <core:Item
                                id="_IDGenItems112111"
                                text="Inbound"
                                key="Inbound"
                            />
                            <core:Item
                                id="_IDGenItems112232"
                                text="Outbound"
                                key="Outbound"
                            />
                        </items>
                    </Select>
                    <!-- <Input
                        id="idDeliveryeditvalue"
                        value="{}"
                        width="55%"
                        editable="true"
                    /> -->
                    <Label
                        id="idSataustslotedit"
                        text="Status"
                        width="55%"
                    />
                    <Text
                        id="idStatusslotchange"
                        text=""
                        width="55%"
                    />
                </f:content>
            </f:SimpleForm>
        </content>
    </Dialog>
</core:FragmentDefinition>
