<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
>
    <Dialog
        id="idDialogCreate"
        resizable="true"
        draggable="true"
        contentWidth="30%"
        title="Create Reservation"
    >
        <beginButton>
            <Button
                id="idCreateButtonnew8"
                text="Create"
                press="onCreateReserveDialog"
                type="Success"
            />
        </beginButton>
        <endButton>
            <Button
                id="idCancelButtonnew12"
                text="Cancel"
                press="onCloseDialog"
                type="Negative"
            />
        </endButton>
        <content>
            <form:SimpleForm
                id="_IDGenSimpleForm1new"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="3"
                labelSpanL="3"
                labelSpanM="3"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="4"
                emptySpanL="4"
                emptySpanM="4"
                emptySpanS="0"
                columnsXL="1"
                columnsL="1"
                columnsM="1"
                singleContainerFullSize="false"
            >
                <Label
                    id="idTruckNumberLabel"
                    text="TruckNumber"
                />
                <Input
                    id="idTruckNumberInput"
                    value="{}"
                    liveChange="TruckLiveChange"
                    required="true"
                />
                <Label
                    id="idDriverNameLabel"
                    text="DriverName"
                />
                <Input
                    id="idDriverNameInput"
                    value="{}"
                    required="true"
                    liveChange="DriverNameChange"
                />
                <Label
                    id="idDriverMobilenumbernew"
                    text="Driver Mobile"
                />
                <Input
                    id="idDrivernewMobile"
                    value="{}"
                    liveChange="onMobileVal"
                    required="true"
                />
                <Label
                    id="idreseVendorname"
                    text="VendorName"
                />
                <Input
                    id="idvessbane"
                    value="{}"
                    required="true"
                />
                <Label
                    id="idinboundlabe"
                    text="Delivery"
                />
                <Select
                    id="inwards21"
                    required="true"
                    selectedKey="{Delivery}"
                >
                    <items>
                        <core:Item
                            id="iditeadms12"
                            text=" "
                            key=""
                        />
                        <core:Item
                            id="_IDGenItems1121"
                            text="Yes"
                            key="true"
                        />
                        <core:Item
                            id="_IDGenItems112"
                            text="No"
                            key="false"
                        />
                    </items>
                </Select>
                <Label id="idddlabel" text="Parking Slot"/>
                <Select
                    id="parkingLotSelect122"
                    required="true"
                    items="{path: '/PARKINGSLOTSSet', filters: [{path: 'Status', operator: 'EQ', value1: 'Available'}]}"
                >
                    <core:Item
                        id="_IDGenItem51"
                        key="{Slotno}"
                        text="{Slotno}"
                    />
                </Select>
            </form:SimpleForm>
        </content>
    </Dialog>
</core:FragmentDefinition>
